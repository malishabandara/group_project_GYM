{
  "expo": {
    "name": "AppWrite",
    "slug": "AppWrite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Loading.png",
    "scheme": "com.laka",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/images/Loading.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Loading.png",
        "backgroundColor": "#000000"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "com.laka",
            "host": "*auth",
            "pathPrefix": "/callback"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
      
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ad9c623-022c-496b-a863-f07fea48a050"
      },
      "googleClientId": "323563589391-nhhc1s2r290qqm6jbtvcjlk77i5o1lsi.apps.googleusercontent.com"
    }
  }
}
